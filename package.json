{
  "name": "clickup-cli-tools",
  "version": "0.3.0",
  "description": "ClickUp CLI tools",
  "author": "Alex House",
  "bin": {
    "clickup": "./bin/run"
  },
  "homepage": "https://github.com/alexhouse/clickup-cli-tools",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "alexhouse/clickup-cli-tools",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.7.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "axios": "^0.26.1",
    "execa": "^6.1.0",
    "fs-extra": "^10.0.1",
    "inquirer": "^8.2.2",
    "listr": "^0.14.3"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/listr": "^0.14.4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "clickup",
    "dirname": "clickup",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "auth": {
        "description": "Authenticate with ClickUp"
      },
      "view": {
        "description": "View a ClickUp ticket"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/alexhouse/clickup-cli-tools/issues",
  "keywords": [
    "clickup"
  ],
  "types": "dist/index.d.ts"
}
